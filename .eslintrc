{
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "no-restricted-syntax": 0,
    "func-names": 0,
    "no-param-reassign": 0,
    "no-console": 0,
    "import/prefer-default-export": 0,
    "semi": [
      "error",
      "never"
    ],
    "arrow-parens": ["error", "as-needed"],
    "arrow-body-style": ["error", "always"]
  }
}
